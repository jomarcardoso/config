{
    // "color-highlight.matchWords": false,

    // "editor.occurrencesHighlight": false,
    "editor.renderLineHighlight": "gutter",
    // "editor.suggestLineHeight": null,
    // "editor.matchBrackets": false,
    // "editor.renderControlCharacters": true,
    "editor.lineHeight": 26,
    "editor.fontSize": 13,
    "editor.fontFamily": "Office Code Pro Medium, Consolas, 'Courier New', monospace",
    // "editor.suggestSelection": "first",
    "editor.colorDecorators": false,
    "editor.tabSize": 2,
    "editor.insertSpaces": true,

    // "emmet.syntaxProfiles": {
    //     "javascript": "jsx",
    // },
    "emmet.triggerExpansionOnTab": true,

    "eslint.alwaysShowStatus": true,
    "eslint.autoFixOnSave": true,

    // "explorer.openEditors.visible": 1,
    "explorer.confirmDelete": false,

    "files.exclude": {
      "**/.classpath": true,
      "**/.factorypath": true,
      "**/.project": true,
      "**/.settings": true,
      "**/package-lock.json": true,
      "**/yarn-error.log": true,
      "**/yarn.lock": true
    },
    // "files.eol": "\r\n",
    "files.trimTrailingWhitespace": true,
    "files.watcherExclude": {
      "**/dist": true,
      "/dist": true
    },

    "git.confirmSync": false,
    "git.autofetch": true,

    "html.format.wrapAttributes": "aligned-multiple",
    "html.format.wrapLineLength": 2000,

    "javascript.updateImportsOnFileMove.enabled": "always",

    "latex-workshop.view.pdf.viewer": "browser",

    // "outline.problems.colors": false,

    "search.exclude": {
      "**/dist": true,
      "**/node_modules": false,
      "**/package-lock.json": true,
      "**/yarn-error.log": true,
      "**/yarn.lock": true,
      "/dist": true
    },
    // "search.useIgnoreFiles": false,

    // "terminal.integrated.shell.windows": "C:\\Program Files\\Git\\bin\\bash.exe",
    // "terminal.integrated.rendererType": "dom",
    "terminal.integrated.fontSize": 13,

    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",

    "window.zoomLevel": 0,

    "workbench.statusBar.feedback.visible": false,
    "workbench.colorTheme": "Monokai One Dark Vivid",
    "workbench.startupEditor": "newUntitledFile",
    "editor.suggestSelection": "first",
    "explorer.confirmDragAndDrop": false,
    "workbench.iconTheme": "material-icon-theme",
    "editor.formatOnPaste": true,
    "terminal.integrated.rendererType": "dom",
    "editor.autoIndent": false,
    "stylelint.autoFixOnSave": true,
    "search.useReplacePreview": false,
    "search.useIgnoreFiles": false,
}